int i, j;
//------------------------------------------------------
int mcd(int a, int b) {
	int i, j, k;

	j = a;
	k = b;
	i = a % b;
	while (i != 0) {
		j = k;
		k = i;
		i = j % k;
	}
	return k;
}
//------------------------------------------------------
void main() {
	read (i);
	read (j);
	print_ln (mcd (i,j));
}
